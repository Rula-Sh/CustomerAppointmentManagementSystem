{
  "version": "3.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "timepicker@1.3.5",
      "destination": "wwwroot/timepicker/",
      "files": [
        "jquery.timepicker.min.js",
        "jquery.timepicker.min.css"
      ]
    },
    {
      "library": "bootstrap-datepicker@1.10.0",
      "destination": "wwwroot/bootstrap-datepicker/",
      "files": [
        "js/bootstrap-datepicker.min.js",
        "css/bootstrap-datepicker.min.css"
      ]
    },
    {
      "library": "bootstrap-icons@1.11.3",
      "destination": "wwwroot/bootstrap-icons/",
      "files": [
        "bootstrap-icons.svg",
        "font/fonts/bootstrap-icons.woff2",
        "font/fonts/bootstrap-icons.woff",
        "font/bootstrap-icons.min.css"
      ]
    },
    {
      "library": "bootstrap@5.3.3",
      "destination": "wwwroot/bootstrap/",
      "files": [
        "js/bootstrap.min.js"
      ]
    },
    {
      "library": "bootbox.js@6.0.3",
      "destination": "wwwroot/bootbox.js/",
      "files": [
        "bootbox.min.js"
      ]
    },
    {
      "library": "toastr.js@2.1.4",
      "destination": "wwwroot/toastr.js/",
      "files": [
        "toastr.min.js",
        "toastr.min.css"
      ]
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@8.0.7",
      "destination": "wwwroot/microsoft/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    }
  ]
}